/boot		emmc		/dev/block/mmcblk0p14
/cache		ext4		/dev/block/mmcblk0p24
/data		ext4		/dev/block/mmcblk0p26
/recovery	emmc		/dev/block/mmcblk0p15
/system		ext4		/dev/block/mmcblk0p23
/external_sd		vfat		/dev/block/mmcblk1p1	/dev/block/mmcblk1 flags=display="Micro SDcard";storage;wipeingui;removable
/efs		emmc		/dev/block/mmcblk0p11 flags=display="EFS";backup=1
/modem		emmc		/dev/block/mmcblk0p2 flags=backup=1;display="Modem";fsflags=ro,context=u:object_r:firmware_file:s0
/usb-otg vfat /dev/block/sda1 /dev/block/sda flags=display="USB-OTG";storage;wipeingui;removable
